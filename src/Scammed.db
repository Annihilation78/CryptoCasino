CREATE TABLE usuarios (
    id INTEGER PRIMARY KEY,
    usuario TEXT NOT NULL UNIQUE,
    contrasena TEXT NOT NULL,
    email TEXT NOT NULL,
    wallet REAL DEFAULT 0
);

