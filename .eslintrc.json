{
    "extends": ["eslint:recommended", "plugin:jsx-a11y/recommended"],
    "plugins": ["jsx-a11y"],
    "rules": {
      "jsx-a11y/alt-text": "error",
      "jsx-a11y/no-distracting-elements": "error",
      "jsx-a11y/accessible-emoji": "error"
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    }
  }
  